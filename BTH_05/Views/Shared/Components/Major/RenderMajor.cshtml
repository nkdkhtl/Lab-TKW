@model IEnumerable<Major>

<ul class="nav">
    @foreach(var m in Model)
    {
        <li class="nav-item" id="@m.MajorID" style="cursor:pointer;">
            <a class="nav-link">
                @m.MajorName
            </a>
        </li>
    }
</ul>
